{
  "name": "metaflac-browser-js",
  "version": "1.1.3",
  "description": "A pure JavaScript implementation of the metaflac (official FLAC tool) for browser",
  "main": "./dist/metaflac-browser-js.esm.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/metaflac-browser-js.esm.js",
  "type": "module",
  "files": [
    "src",
    "dist",
    "docs",
    "README.md",
    "public"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/metaflac-browser-js.esm.js",
      "import": "./dist/metaflac-browser-js.esm.js",
      "require": "./dist/metaflac-browser-js.js",
      "default": "./dist/metaflac-browser-js.esm.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build && tsc --emitDeclarationOnly --declaration",
    "build:demo": "vite build --config vite.config.demo.ts",
    "build:watch": "vite build --watch",
    "prepublishOnly": "pnpm run type-check && pnpm run lint && pnpm run build",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "lint": "pnpm run type-check && pnpm run format:check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/520Qiuyu/metaflac-browser-js.git"
  },
  "keywords": [
    "metaflac",
    "flac",
    "metadata",
    "audio",
    "browser",
    "vorbis",
    "comment",
    "tag",
    "picture",
    "cover",
    "browser-only"
  ],
  "author": "520Qiuyu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/520Qiuyu/metaflac-browser-js/issues"
  },
  "homepage": "https://github.com/520Qiuyu/metaflac-browser-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "prettier": "^3.6.2",
    "rollup": "^4.9.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-legacy": "^7.2.1"
  },
  "unpkg": "dist/metaflac-browser-js.js",
  "jsdelivr": "dist/metaflac-browser-js.js",
  "dependencies": {}
}
